public class Print_2d_Array {

	public static void main(String[] args) {
		
		//2d Array
		int Array[][] = { { 1, 2 }, { 3, 4 }, { 5, 6 } };

		// matrix=3*2

		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 2; j++) {
				System.out.print(Array[i][j] + " ");
			}
			System.out.println();
		}
	}
}
