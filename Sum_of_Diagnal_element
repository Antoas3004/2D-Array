public class Sum_of_Diagnal_element {

	public static void main(String[] args) {

		int sum = 0;
		int a[][] = { { 9, 5, 7 }, { 1, 2, 3 }, { 4, 6, 8 } };
		/*
		 * diagnal element 9,2,8 
		 * sum of diagnal element =9+2+8 
		 * sum=19
		 */

		for (int i = 0; i < a.length; i++) {
			for (int j = 0; j < a[i].length; j++) {
				if (i == j) {
					sum = sum + a[i][j];
				}
			}
		}
		System.out.println("Sum of Diagnal element = " + sum);
	}
}
